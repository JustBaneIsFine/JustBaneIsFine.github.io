{
    "name": "back-end",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "tsc-format": "tsc && npm run format",
        "lint": "eslint --fix --ext .js,.ts .",
        "format": "prettier --write \"**/*.+(js|ts|json)\"",
        "lint-pretty": "npm run lint && npm run format",
        "lpt": "npm run-s -c lint-pretty tsc-format",
        "startPure": "node server.js",
        "fix-start": "npm run lpt && npm run startPure",
        "start": "npm run fix-start",
        "test": "jest"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "^2.6.9",
        "express": "^4.1.1",
        "http-errors": "^1.6.3",
        "mongodb": "^4.10.0"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.3",
        "@babel/preset-typescript": "^7.18.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/express": "^4.17.14",
        "@types/jest": "^29.0.3",
        "@types/node": "^18.7.23",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "babel-jest": "^29.1.0",
        "depcheck": "^1.4.3",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "typescript": "^4.8.4"
    }
}
