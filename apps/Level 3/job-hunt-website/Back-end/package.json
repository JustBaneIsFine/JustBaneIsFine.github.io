{
    "name": "back-end",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "tsc-format": "tsc && npm run format",
        "lint": "eslint --fix --ext .js,.ts .",
        "format": "prettier --write \"**/*.+(js|ts|json)\"",
        "lint-pretty": "npm run lint && npm run format",
        "lpt": "npm run-s -c lint-pretty tsc-format",
        "startPure": "node out/server.js",
        "fix-start": "npm run lpt && npm run startPure",
        "start": "ts-node server.ts",
        "test": "jest ./tests"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "^2.6.9",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "http-errors": "^1.6.3",
        "mongodb": "^4.10.0"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/http-errors": "^2.0.0",
        "@types/jest": "^29.1.2",
        "@types/node": "^18.8.5",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "babel-jest": "^29.1.2",
        "depcheck": "^1.4.3",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.1.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
